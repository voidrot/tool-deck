{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "splashscreen"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-spawn",
    "log:default",
    "notification:default",
    "store:default",
    "positioner:default",
    "process:default",
    "fs:default",
    "system-info:allow-all"
  ]
}