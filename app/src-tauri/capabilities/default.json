{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main",
    "splashscreen"
  ],
  "permissions": [
    "core:default",
    "shell:allow-open",
    "shell:allow-spawn",
    "fs:default",
    "log:default",
    "notification:default",
    "store:default",
    "websocket:default",
    "cli:default",
    "positioner:default",
    "process:default",
    "system-info:allow-all",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://*"
        },
        {
          "url": "http://*"
        }
      ],
      "deny": []
    }
  ]
}
